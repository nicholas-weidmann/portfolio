---
import NavbarLayout from '../layouts/NavbarLayout.astro'
---

<NavbarLayout title="Nicholas Weidmann">
  <div class="card w-96 bg-base-200 shadow-xl">
    <a href="/projects/test_client">
      <figure>
        <img src="/images/mmocld-test_client.png" alt="mmocld test_client" transition:name="img-mmocld-test_client" />
      </figure>
      <div class="card-body">
        <h2 class="card-title">
          Test Client
          <div class="badge badge-secondary">mmocld</div>
        </h2>
        <p>If a dog chews shoes whose shoes does he choose?</p>
        <div class="card-actions justify-end font-semibold">
          <div class="badge badge-accent">Angular</div>
          <div class="badge badge-primary">API</div>
          <div class="badge badge-primary">OpenID Connect</div>
        </div>
      </div>
    </a>
  </div>
</NavbarLayout>
