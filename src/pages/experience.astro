<NavbarLayout title="Nicholas Weidmann">
  <div class="flex justify-center">
    <ul class="timeline timeline-vertical timeline-snap-icon max-lg:timeline-compact">
      <li>
        <div class="timeline-middle">
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="h-5 w-5"
            ><path
              fill-rule="evenodd"
              d="M10 18a8 8 0 100-16 8 8 0 000 16zm3.857-9.809a.75.75 0 00-1.214-.882l-3.483 4.79-1.88-1.88a.75.75 0 10-1.06 1.061l2.5 2.5a.75.75 0 001.137-.089l4-5.5z"
              clip-rule="evenodd"></path></svg
          >
        </div>
        <div class="timeline-start">
          <div class="card mb-10 bg-base-200 shadow-xl lg:mb-0">
            <div class="card-body indicator w-auto min-w-0 max-w-[26rem]">
              <span
                class="badge indicator-item badge-secondary badge-outline indicator-center indicator-top font-semibold"
              >
                Jan 2023 - Present
              </span>
              <h2 class="card-title">System Engineer</h2>
              <h3 class="link-hover link-info link">@ SIX Group</h3>
